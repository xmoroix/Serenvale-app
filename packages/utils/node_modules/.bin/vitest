#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Serenvale/node_modules/.pnpm/vitest@3.2.4_@edge-runtime+vm@5.0.0_@types+debug@4.1.12_@types+node@24.10.0_happy-dom@2_48e438cc5bfbc9410fe4ce29b7e8bbee/node_modules/vitest/node_modules:/home/user/Serenvale/node_modules/.pnpm/vitest@3.2.4_@edge-runtime+vm@5.0.0_@types+debug@4.1.12_@types+node@24.10.0_happy-dom@2_48e438cc5bfbc9410fe4ce29b7e8bbee/node_modules:/home/user/Serenvale/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Serenvale/node_modules/.pnpm/vitest@3.2.4_@edge-runtime+vm@5.0.0_@types+debug@4.1.12_@types+node@24.10.0_happy-dom@2_48e438cc5bfbc9410fe4ce29b7e8bbee/node_modules/vitest/node_modules:/home/user/Serenvale/node_modules/.pnpm/vitest@3.2.4_@edge-runtime+vm@5.0.0_@types+debug@4.1.12_@types+node@24.10.0_happy-dom@2_48e438cc5bfbc9410fe4ce29b7e8bbee/node_modules:/home/user/Serenvale/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vitest@3.2.4_@edge-runtime+vm@5.0.0_@types+debug@4.1.12_@types+node@24.10.0_happy-dom@2_48e438cc5bfbc9410fe4ce29b7e8bbee/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vitest@3.2.4_@edge-runtime+vm@5.0.0_@types+debug@4.1.12_@types+node@24.10.0_happy-dom@2_48e438cc5bfbc9410fe4ce29b7e8bbee/node_modules/vitest/vitest.mjs" "$@"
fi
