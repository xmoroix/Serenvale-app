#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Serenvale/node_modules/.pnpm/officeparser@5.1.1/node_modules/officeparser/node_modules:/home/user/Serenvale/node_modules/.pnpm/officeparser@5.1.1/node_modules:/home/user/Serenvale/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Serenvale/node_modules/.pnpm/officeparser@5.1.1/node_modules/officeparser/node_modules:/home/user/Serenvale/node_modules/.pnpm/officeparser@5.1.1/node_modules:/home/user/Serenvale/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../officeparser/officeParser.js" "$@"
else
  exec node  "$basedir/../officeparser/officeParser.js" "$@"
fi
