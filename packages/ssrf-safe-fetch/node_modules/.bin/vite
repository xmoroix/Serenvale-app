#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/Serenvale/node_modules/.pnpm/vite@7.2.2_@types+node@24.10.0_jiti@2.6.1_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/node_modules:/home/user/Serenvale/node_modules/.pnpm/vite@7.2.2_@types+node@24.10.0_jiti@2.6.1_tsx@4.20.6_yaml@2.8.1/node_modules/vite/node_modules:/home/user/Serenvale/node_modules/.pnpm/vite@7.2.2_@types+node@24.10.0_jiti@2.6.1_tsx@4.20.6_yaml@2.8.1/node_modules:/home/user/Serenvale/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/Serenvale/node_modules/.pnpm/vite@7.2.2_@types+node@24.10.0_jiti@2.6.1_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/node_modules:/home/user/Serenvale/node_modules/.pnpm/vite@7.2.2_@types+node@24.10.0_jiti@2.6.1_tsx@4.20.6_yaml@2.8.1/node_modules/vite/node_modules:/home/user/Serenvale/node_modules/.pnpm/vite@7.2.2_@types+node@24.10.0_jiti@2.6.1_tsx@4.20.6_yaml@2.8.1/node_modules:/home/user/Serenvale/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@7.2.2_@types+node@24.10.0_jiti@2.6.1_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@7.2.2_@types+node@24.10.0_jiti@2.6.1_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
